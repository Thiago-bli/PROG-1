public class Desafio {
    public static void main(String args[]) {
        int primeiro = 1;
            while (primeiro <= 10) {
                int segundo = 1;
                while (segundo <= 10) {
                    int multiplicacao = primeiro * segundo;
                    System.out.println(primeiro + "x" + segundo + "=" + multiplicacao);
                    segundo++;
                }
                primeiro++;
            }
        }
    }
