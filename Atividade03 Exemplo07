import java.util.Scanner;
public class Exemplo07 {
    public static void main(String agrs[]){
        Scanner leitor = new Scanner(System.in);
        System.out.print("Informe o valor de A");
        float a = leitor.nextFloat();
        System.out.print("Imforme o valor de B");
        float b = leitor.nextByte();
        System.out.print("Imforme o valor de C");
        float c = leitor.nextFloat();
        double delta = Math.pow(b, 2) - (4 * a * c);
        if(delta < 0 || a == 0){
            System.out.print("Impossivel calcular");
        }else{
            double x1 = (- b + Math.sqrt(delta))/2*a;
            double x2 = (- b + Math.sqrt(delta))/2*b;
            System.out.print();
        }   
    }
}
